#intro h1 {
	padding-left: 53vh;
	padding-top: 30vh;
	white-space: pre;
}

#intro h1 strong::before {
	content: "\A";
}

#intro h1 strong::after {
	content: "\A";
}

#intro .excluder {
	width: 200px;
	height: 200px;
	-webkit-shape-outside: polygon(0 0, 100% 0, 0 100%);
	float: left;
	margin-left: 50vh;
	margin-top: 29vh;
}

#intro .company {
	background: $secondaryBackgroundColor;
	display: inline-block;
	height: 100vh;
	position: absolute;
	top: 0;
	left: 0;
	bottom: 0;
	padding-right: 100vh;
	/*-webkit-clip-path: polygon(0 0, -webkit-calc(60px + 11%) 0%, 50% 100%, 0% 100%);*/
	-webkit-mask: url("/icon/sap_mask.svg") top left / cover;
	z-index: 2;
}

#intro .company img.sap {
	position: absolute;
	bottom: 80px;
	left: 20px;
}

#intro .company img.dhbw {
	position: absolute;
	top: 20px;
	left: 20px;
}

#intro .contact {
	position: absolute;
	bottom: 80px;
	left: 0;
	height: 60px;
	line-height: 60px;
	overflow: hidden;
	padding-left: 191px;
	padding-right: 80px;
	border-top: 1px solid $mainColor;
	border-bottom: 1px solid $mainColor;
	box-sizing: border-box;
	-webkit-mask: url("/icon/sap_mask.svg") top left / cover;
}

#intro .contact::after {
	content: "";
	position: absolute;
	top: 0;
	right: 30px;
	width: 1px;
	height: 82px;
	margin-top: -12px;
	margin-left: 30px;
	background: #004C67;
	-webkit-transform: rotate(45deg);
}

#intro .contact > * {
	display: inline-block;
	padding: 0 25px;
}

#intro .contact > *:not(:last-child)::after {
	content: "";
	position: absolute;
	top: 0;
	width: 1px;
	height: 20px;
	margin-top: 20px;
	margin-left: 30px;
	background: $mainColor;
	-webkit-transform: rotate(45deg);
}

#journeyimages .container {
	height: 600px;
	white-space: nowrap;
	position: absolute;
	top: 50%;
	margin-top: -300px;
	background: $secondaryBackgroundColor;
}

#journeyimages .container img {
	height: 600px;
	width: auto;
	opacity: 0;
}

#journeyimages .container img.active {
	opacity: 1;
}

#resultsUshahidi > div, #picnic > div {
	display: -webkit-flex;
	-webkit-align-items: center;
	-webkit-justify-content: space-around;
	height: 100%;
}

#resultsUshahidi .multipleStarPlots .axis .legend {
	fill: red;
